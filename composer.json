{
    "name": "aoe/typo3-cli-tools",
    "description": "Provide cli tools to manipulate TYPO3 data.",
    "homepage": "https://github.com/AOEpeople/TYPO3-CLI-Tools",
    "license": "GPL-3.0",
    "support": {
        "email": "dev@aoe.com",
        "issues": "https://github.com/AOEpeople/TYPO3-CLI-Tools/issues",
        "source": "https://github.com/AOEpeople/TYPO3-CLI-Tools"
    },
    "authors": [
        {
            "name": "AOE GmbH",
            "email": "dev@aoe.com",
            "homepage": "http://www.aoe.com"
        }
    ],
    "bin": [
        "bin/typo3-cli-tools"
    ],
    "require": {
        "php": "^7.0",
        "symfony/console": "^2.7",
        "typo3/cms-core": ">=7.6.0,<=8.7.99",
        "typo3/cms-extensionmanager": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5",
        "mikey179/vfsstream": "^1.5"
    },
    "autoload": {
        "psr-4": {
            "AOE\\TYPO3CLITools\\": "src/"
        }
    },
    "replace": {
        "typo3/cms-composer-installers": "*"
    },
    "keywords": [
        "GIT",
        "Subversion",
        "VCS",
        "Tag",
        "Tagging",
        "automatic",
        "build"
    ]
}
